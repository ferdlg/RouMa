CREATE PROCEDURE CLIENTES_BOGOTA()
SELECT ID_CLIENTE, NOMBRE_CLIENTE, EMAIL
FROM clientes WHERE CIUDAD = 'BOGOTA'

CALL CLIENTES_BOGOTA()

estructura completa
Procedimiento para saber la edad

DELIMITER $$
CREATE PROCEDURE CALCULAR_EDAD (IN Aﾃ前_NACIMIENTO INT)

BEGIN 
  DECLARE Aﾃ前_ACTUAL INT DEFAULT 2023;
  DECLARE EDAD INT;
  SET EDAD+Aﾃ前_ACTUAL+Aﾃ前_NACIMIENTO;
  SELECT EDAD;
END; $$

Procedimiento para suma y promedios de los libros teniendo como entrada el nombre del autor

DELIMITER $$
CREATE PROCEDURE TOTAL_PRECIO(IN NOMBRE_AUTOR VARCHAR(30), OUT SUMA DECIMAL(5,2), OUT PROMEDIO DECIMAL(5,2))

BEGIN 
  SELECT TITULO, EDITORIAL, PRECIO
  FROM libros
  WHERE AUTOR= NOMBRE_AUTOR;
  SELECT SUM (PRECIO) INTO SUMA
  FROM libros
  WHERE AUTOR=NOMBRE_AUTOR;
  SELECT AVG(PRECIO) INTO PROMEDIO
  FROM libros
  EHERE AUTRO= NOMBRE_AUTOR
